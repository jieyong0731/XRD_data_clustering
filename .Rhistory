library(tidyr)
library(dplyr)
## load all the data
X_train <- read.table("./UCI HAR Dataset/train/X_train.txt")
y_train <- read.table("./UCI HAR Dataset./train/y_train.txt")
subject_train <- read.table("./UCI HAR Dataset/train/subject_train.txt")
X_test <- read.table("./UCI HAR Dataset/test/X_test.txt")
y_test <- read.table("./UCI HAR Dataset/test/y_test.txt")
subject_test <- read.table("./UCI HAR Dataset/test/subject_test.txt")
## Calculate the means and standard deviations for each file.
meanXtrain <-rowMeans(X_train, na.rm = TRUE)
meanXtest <-rowMeans(X_test, na.rm = TRUE)
SDXtest <-apply(X_test,1, sd, na.rm = TRUE)
SDXtrain <-apply(X_train,1, sd, na.rm = TRUE)
## combine subject ID, test name, mean and standard deviation for test and train respectively
test <-cbind(subject_test,y_test,meanXtest,SDXtest)
train <-cbind(subject_train,y_train,meanXtrain,SDXtrain)
##rename each column then combine test and train
names(test) <- c("ID","Activity","mean","SD")
names(train) <- c("ID","Activity","mean","SD")
total <- rbind(test,train)
## calculate the mean and SD for each ID and each activity
tidymean <- with(total, tapply(mean, list(ID, Activity), mean))
tidySD <- with(total, tapply(SD, list(ID, Activity), mean))
tidymean <-as.data.frame(tidymean)
tidymean <-cbind(1:length(unique(total$ID)),tidymean)
tidySD <-as.data.frame(tidySD)
tidySD <-cbind(1:length(unique(total$ID)),tidySD)
names <-c("WALKING","WALKING_UPSTAIRS","WALKING_DOWNSTAIRS","SITTING","STANDING","LAYING")
names(tidymean) <- c("ID",names)
names(tidySD) <- c("ID",names)
tidymean <-gather(tidymean,Activity,names,-ID)
names(tidymean)[3] <- "mean"
tidySD <-gather(tidySD,Activity,names,-ID)
names(tidySD)[3] <- "SD"
tidy <- merge(tidymean,tidySD,all=TRUE)
tidy <-arrange(tidy,ID, Activity)
tail(tidy)
## write the files to local computer disk
write.table(tidy, file = "tidy.txt", row.name=FALSE)
